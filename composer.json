{
  "name": "api-client-darsky/darksky-client",
  "description": "Dark sky simple api client",
  "homepage": "https://github.com/jorgik1/darksky-client",
  "type": "library",
  "require": {
    "php": "^7.1",
    "api-clients/client-services": "^1.1",
    "api-clients/foundation": "^1.0",
    "api-clients/middleware-json": "^1.0",
    "api-clients/middleware-user-agent": "^1.0"
  },
  "require-dev": {
    "api-clients/resource-generator": "dev-master",
    "api-clients/resource-test-utilities": "^1.0",
    "api-clients/test-utilities": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "ApiClients\\Client\\DarkSkyClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApiClients\\Tests\\Client\\DarkSkyClient\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Yuriy Stenin",
      "email": "yuriy.ipcom@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "extra": {
    "api-clients": {
      "command-bus": {
        "path": "src/CommandBus",
        "namespace": "ApiClients\\Client\\DarkSkyClient\\CommandBus"
      }
    }
  }
}
